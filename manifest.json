{
    "manifest_version": 3,
    "name": "YouTube Tab Watcher",
    "version": "1.0",
    "permissions": [
        "tabs",
        "webNavigation",
        "activeTab",
        "scripting"
    ],

    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "128": "icons/icon128.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "128": "icons/icon128.png"
    }
  }
  